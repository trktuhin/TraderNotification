{
    "manifest_version": 3,
    "name": "Trader Notification",
    "description": "Automatically reloads the current tab and checks for new data.",
    "version": "1.2.3",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}